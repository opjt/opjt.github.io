{
	"plugins": ["prettier-plugin-go-template", "prettier-plugin-tailwindcss"],
	"htmlWhitespaceSensitivity": "css",
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 120,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": ["*.html"],
			"options": {
				"parser": "go-template"
			}
		}
	]
}
